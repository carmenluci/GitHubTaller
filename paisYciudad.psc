Proceso paisYciudad
	Definir ciudad Como Caracter;
	Definir pais Como Caracter;
	
	ciudad <- "Canberra";
	pais <- "Australia";
	
	Escribir "¿Qué país te gustaría conocer?";
	Leer pais;
	Escribir "¿Cuál es la capital de ese país?";
	Leer ciudad;
	
	Limpiar Pantalla;
	
	Escribir "La ciudad ", ciudad, " ", "es la capital del país ", pais, " ", "y me encantaría conocerla";
	
FinProceso
